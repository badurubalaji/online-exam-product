<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="com.onlineexam.model">
  <class name="Question" table="question">
	<id name="questionId" type="long" column="question_id">
		<generator class="increment"/>  	
  	</id>
  	<property name="questionText" column="question_text" type="string"/>
  	<property name="optionOne" column="option_one" type="string"/>
  	<property name="optionTwo" column="option_two" type="string"/>
  	<property name="optionThree" column="option_three" type="string"/>
  	<property name="optionFour" column="option_four" type="string"/>
  	<property name="correctOpitons" column="correct_options" type="string"/>
  	<property name="level" column="level" type="string"/>
  	
  </class>
</hibernate-mapping>