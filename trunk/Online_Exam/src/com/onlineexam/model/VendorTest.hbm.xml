<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onlineexam.model.VendorTest" table="vendor_test">
		
		<composite-id name="vendorTestCompositeId"
			class="com.onlineexam.model.VendorTestCompositeId">

			<key-property name="vendor" column="vendor_id"
				type="java.lang.Integer" />
			<key-property name="student" column="student_id"
				type="java.lang.Integer" />
			<key-property name="test" column="test_id" type="java.lang.Integer" />

		</composite-id>
		<property name="purchaseDate" column="purchase_date" />
		<property name="writtenDate" column="written_date" />
		<property name="assignDate" column="assign_date" />
		<property name="result" column="result" />
		<many-to-one name="vendorTests" column="vendor_id" />
		<many-to-one name="studentTests" column="student_id" />



	</class>

</hibernate-mapping>
